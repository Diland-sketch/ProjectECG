<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:GUI.ViewModel"
                    xmlns:view="clr-namespace:GUI.ViewDashBoard">
    
    
    <DataTemplate DataType="{x:Type vm:ECGMonitoringVM}">
        <view:ECGMonitoringView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:HomeView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PatientHistoryVM}">
        <view:PatientHistoryView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PatientRegistrationVM}">
        <view:PatientRegistrationView />
    </DataTemplate>

    
    
    
    
</ResourceDictionary>
